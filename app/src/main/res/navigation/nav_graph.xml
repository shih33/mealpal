            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_whoFragment_to_findfriendsetFragment"
            app:destination="@id/findfriendsetFragment" />
    </fragment>
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.example.meal_kotlin.FavoriteFragment"
        android:label="fragment_favorite"
        tools:layout="@layout/fragment_favorite" />
    <fragment
        android:id="@+id/findfriendsetFragment"
        android:name="com.example.meal_kotlin.FindfriendsetFragment"
        android:label="fragment_findfriendset"
        tools:layout="@layout/fragment_findfriendset" >
        <action
            android:id="@+id/action_findfriendsetFragment_to_invitefriendFragment"
            app:destination="@id/invitefriendFragment" />
    </fragment>
    <fragment
        android:id="@+id/invitefriendFragment"
        android:name="com.example.meal_kotlin.InvitefriendFragment"
        android:label="fragment_invitefriend"
        tools:layout="@layout/fragment_invitefriend" >
        <action
            android:id="@+id/action_invitefriendFragment_to_manageFragment"
            app:destination="@id/manageFragment" />
    </fragment>
    <fragment
        android:id="@+id/invitefinishFragment"
        android:name="com.example.meal_kotlin.InvitefinishFragment"
        android:label="fragment_invitefinish"
        tools:layout="@layout/fragment_invitefinish" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.meal_kotlin.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/t1Fragment"
        android:name="com.example.meal_kotlin.T1Fragment"
        android:label="fragment_t1"
        tools:layout="@layout/fragment_t1" />
    <fragment
        android:id="@+id/personFragment"
        android:name="com.example.meal_kotlin.PersonFragment"
        android:label="fragment_person"
        tools:layout="@layout/fragment_person" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.meal_kotlin.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/messageFragment"
        android:name="com.example.meal_kotlin.MessageFragment"
        android:label="fragment_message"
        tools:layout="@layout/fragment_message" />
</navigation>