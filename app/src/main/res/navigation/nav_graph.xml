<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.meal_kotlin.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.meal_kotlin.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_findrtFragment"
            app:destination="@id/findrtFragment" />
        <action
            android:id="@+id/action_homeFragment_to_whoFragment"
            app:destination="@id/whoFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.meal_kotlin.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment_to_voteFragment"
            app:destination="@id/voteFragment" />
    </fragment>
    <fragment
        android:id="@+id/voteFragment"
        android:name="com.example.meal_kotlin.VoteFragment"
        android:label="fragment_vote"
        tools:layout="@layout/fragment_vote" >
        <action
            android:id="@+id/action_voteFragment_to_vresultFragment"
            app:destination="@id/vresultFragment" />
    </fragment>
    <fragment
        android:id="@+id/vresultFragment"
        android:name="com.example.meal_kotlin.VresultFragment"
        android:label="fragment_vresult"
        tools:layout="@layout/fragment_vresult" >
        <action
            android:id="@+id/action_vresultFragment_to_manageFragment"
            app:destination="@id/manageFragment" />
    </fragment>
    <fragment
        android:id="@+id/invitedFragment"
        android:name="com.example.meal_kotlin.InvitedFragment"
        android:label="fragment_invited"
        tools:layout="@layout/fragment_invited" />
    <fragment
        android:id="@+id/manageFragment"
        android:name="com.example.meal_kotlin.ManageFragment"
        android:label="fragment_manage"
        tools:layout="@layout/fragment_manage" >
        <action
            android:id="@+id/action_manageFragment_to_invitationFragment"
            app:destination="@id/invitationFragment" />
    </fragment>
    <fragment
        android:id="@+id/invitationFragment"
        android:name="com.example.meal_kotlin.InvitationFragment"
        android:label="fragment_invitation"
        tools:layout="@layout/fragment_invitation" >
        <action
            android:id="@+id/action_invitationFragment_to_manageFragment"
            app:destination="@id/manageFragment" />
    </fragment>
    <fragment
        android:id="@+id/findrtFragment"
        android:name="com.example.meal_kotlin.FindrtFragment"
        android:label="fragment_findrt"
        tools:layout="@layout/fragment_findrt" >
        <action
            android:id="@+id/action_findrtFragment_to_gachaFragment"
            app:destination="@id/gachaFragment" />
    </fragment>
    <fragment
        android:id="@+id/gachaFragment"
        android:name="com.example.meal_kotlin.GachaFragment"
        android:label="fragment_gacha"
        tools:layout="@layout/fragment_gacha" >
        <action
            android:id="@+id/action_gachaFragment_to_getrtFragment"
            app:destination="@id/getrtFragment" />
    </fragment>
    <fragment
        android:id="@+id/getrtFragment"
        android:name="com.example.meal_kotlin.GetrtFragment"
        android:label="fragment_getrt"
        tools:layout="@layout/fragment_getrt" >
        <action
            android:id="@+id/action_getrtFragment_to_listrtFragment"
            app:destination="@id/listrtFragment" />
        <action
            android:id="@+id/action_getrtFragment_to_gachaFragment"
            app:destination="@id/gachaFragment" />
    </fragment>
    <fragment
        android:id="@+id/listrtFragment"
        android:name="com.example.meal_kotlin.ListrtFragment"
        android:label="fragment_listrt"
        tools:layout="@layout/fragment_listrt" />
