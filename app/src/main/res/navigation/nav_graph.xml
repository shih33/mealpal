        android:id="@+id/friendFragment"
        android:name="com.example.meal_kotlin.FriendFragment"
        android:label="fragment_friend"
        tools:layout="@layout/fragment_friend" >
        <action
            android:id="@+id/action_friendFragment_to_buddyFragment"
            app:destination="@id/buddyFragment" />
        <action
            android:id="@+id/action_friendFragment_to_messageFragment"
            app:destination="@id/messageFragment" />
    </fragment>
    <fragment
        android:id="@+id/buddyFragment"
        android:name="com.example.meal_kotlin.BuddyFragment"
        android:label="fragment_buddy"
        tools:layout="@layout/fragment_buddy" >
        <action
            android:id="@+id/action_buddyFragment_to_friendFragment"
            app:destination="@id/friendFragment" />
        <action
            android:id="@+id/action_buddyFragment_to_personFragment"
            app:destination="@id/personFragment" />
    </fragment>
    <fragment
        android:id="@+id/whoFragment"
        android:name="com.example.meal_kotlin.WhoFragment"
        android:label="fragment_who"
        tools:layout="@layout/fragment_who" >
        <action
